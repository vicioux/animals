platform :ios, '9.0'
use_frameworks!

def default_pods
    # cache data
    # https://github.com/Haneke/HanekeSwift
    pod 'HanekeSwift'
    # object mapper
    # https://github.com/bryx-inc/BRYXBanner
    pod 'BRYXBanner'

    pod 'UIScrollView-InfiniteScroll'
end

target 'animals' do
    default_pods
end
